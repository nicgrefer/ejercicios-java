package Practica1_ContraseÃ±as;

import java.security.SecureRandom;
import java.util.Random;


public class Pruevas {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Random aleatorio=new Random();
		int tamanio=aleatorio.nextInt(4,8);
		int contrasenia=0;
		for (int i =0; tamanio >=i;i++) {
			SecureRandom aleatorio1=new SecureRandom();
			int digito=aleatorio1.nextInt(0,9);
			contrasenia=contrasenia+digito;
			}
		System.out.print(contrasenia);
		}
}

