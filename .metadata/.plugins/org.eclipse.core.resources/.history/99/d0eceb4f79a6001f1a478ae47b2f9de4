package clasesextra;

public class EsLetra_ejemplo19_11 {

    public static void main(String[] args) {
        char letra = 'A';
        System.out.print(letra);
        boolean esLetra = true;

        while (esLetra) {
            if ((letra >= 'A' && letra <= 'Z') || (letra >= 'a' && letra <= 'z')) {
                letra++;
                System.out.print(letra);
            } else {
                esLetra = false;
            }
        }
    }
}
