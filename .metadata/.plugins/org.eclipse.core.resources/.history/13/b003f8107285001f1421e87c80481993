package clasesextra;

import java.util.Scanner;

public class ej1 {

	public static void main(String[] args) {
		ejer1();
	}
	
	
	public static void ejer1() { 
			int i=0;
			int j=0;
			while(i<10) {
				while(j<=i) {
					System.out.print("*");
					j++;
				}
			System.out.println();
			j=0;
			i++;
			}
	}
	
	
	public static void ejer2() {
		int dias=1;
		while (dias<=31)
		{
			System.out.println(dias);
			dias++;
		}
	
	}	