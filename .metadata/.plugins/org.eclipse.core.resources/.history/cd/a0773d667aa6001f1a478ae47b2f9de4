package clasesextra;

public class EsLetra_ejemplo19_11 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char letra = '3';

		boolean esLetra=true;
		
		while(esLetra==true) {
			if ((letra >= 64 && letra <= 90) || (letra >= 97 && letra <= 122)) {
				
				System.out.print(letra);
				esLetra=true;
			} else {
				System.out.print("No es una letra");
				esLetra=false;
			}
			letra++;
		}
	}

}
