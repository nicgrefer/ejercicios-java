package clasesextra;

public class b {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String datos[]= {"Javier Vergel","Jony Alonso","Mar Gonzalez","Ana Blanco"};
		int posicion = -1; // Start searching from the beginning of the string

        do {
            posicion = datos.indexOf(" ", posicion + 1);
            System.out.println("Ocurrencia en posici√≥n: " + posicion);
           
        } while (posicion != -1);
		
	}

}
