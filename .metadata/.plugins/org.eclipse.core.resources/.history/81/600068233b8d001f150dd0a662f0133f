package T3ejemplos;

public class Variaciones {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 int n = 4; // NÃºmero de elementos
	        int[] arr = {1, 2, 3, 4}; // Elementos a usar en las variaciones

	        for (int i = 0; i <= n; i++) {
	        	int aux=i;
	            for (int j = 1; j <= n; j++) {
	            	int aux1=j;
	                for (int k = 1; k <= n; k++) {
	                	int aux2=j;
	                    for (int l = 1; l <= n; l++) {
	                    	int aux3=j;
	                       System.out.print(aux+aux1+aux2+aux3);
	                    }
	                }
	            }
	        }
	    }
	}
	}

}
