package clasesextra;

public class Persona {
	String nombre;
	double altura;
	double peso;
	int edad;
	
	public  Persona() {
		
	}

	public Persona(String nombre, double altura, double peso, int edad) { //Constructor crea un objeto
		super();
		this.nombre = nombre;
		this.altura = altura;
		this.peso = peso;
		this.edad = edad;
	}
	
	
	
	
}


