package T3ejemplos;

public class Triangulo1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 int n = 4; // Puedes cambiar este valor para obtener un tri치ngulo m치s grande o m치s peque침o
	        for (int i = 1; i <= n; i++) {
	            for (int j = 1; j <= i; j++) {
	                System.out.print(j + " ");
	            }
	            System.out.println();
	        }
	}
}
