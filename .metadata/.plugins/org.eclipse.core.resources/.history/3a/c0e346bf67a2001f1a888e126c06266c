package T5ejemplos;

public class MeodosRecursividad_V2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		MeodosRecursividad_V2 app=new MeodosRecursividad_V2();
		
		System.out.print(app.factorial(5));
		
	}

	public int factorial (int n) {
		if (n==1)return 1;
		return n*factorial(n-1);
		
	}
	
}
