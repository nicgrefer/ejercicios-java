package T3ejemplos;

public class Variaciones {

	public static void main(String[] args) {
int n = 4; // NÃºmero de elementos
	      
	for (int i = 0; i < n; i++) 
	{
		for (int j = 0; j < n; j++)
		{
			for (int k = 0; k < n; k++)
			{
				for (int l = 0; l < n; l++)
				{
					System.out.println(i+j+k+l);
	            }
	        }
	    }
	}
	}
}
