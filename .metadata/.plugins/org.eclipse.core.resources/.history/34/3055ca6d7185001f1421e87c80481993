package clasesextra;

public class ej1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		ejer2();
	}


	public static void ejer1() {
		int i=0;
		int j=0;
		while(i<10) {
			while(j<=i) {
				System.out.print("*");
				j++;
			}
			System.out.println();
			j=0;
			i++;
		}
		
	}
	
	}	
	public static void ejer2() {
		int dias=0;
		while (dias<30)
		{
			System.out.println(dias);
			dias++;
		}
	
	}

//Mes de enero
		